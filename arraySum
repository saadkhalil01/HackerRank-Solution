# HackerRank-Solution
Its my solution for Problem Solving.
#include<iostream>
using namespace std;

int sumArray(int *arr,const int size){
    int sum=0;
    for(int i=0;i<size;i++){
        sum =sum+arr[i];
    }
    return sum;
}
int main(){
    int size;
    cin>>size;
    int array[size];
    for(int i=0;i<size;i++){
        cin>>array[i];
    }
    cout<<sumArray(array,size);
}
